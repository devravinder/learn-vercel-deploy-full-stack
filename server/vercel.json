{
  "version": 2,
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "devCommand": "pnpm run dev",
  "installCommand": "pnpm install",
  "framework":"elysia",
 "rewrites": [
    { "source": "/", "destination": "/api" },
    { "source": "/api/(.*)", "destination": "/index.js" },
    { "source": "/(.*)",      "destination": "/index.js" }
  ]
}